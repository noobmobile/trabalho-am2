<style>
    .team {
        list-style-type: none;
        display: flex;
        flex-direction: row;
        justify-content: center;
    }
    .team li {
        margin: 0 20px;
    }
    .dev {
        display: flex;
        justify-content: center;
        flex-direction: column;
        align-items: center;
        padding: 10px;
        border-radius: 5px;
        box-shadow: 0 0 3px #999;
    }
    .dev-photo img {
        width: 100%;
        height: 100%;
        border-radius: 50%;
    }
    .dev-photo {
        width: 250px;
        height: 250px;
    }
    .dev-info {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }
    .team-title {
        text-align: center;
        margin-bottom: 20px;
    }
    .license {
        float: right;
    }
</style>
<div>
    <h1 class="team-title">Equipe</h1>
    <ul class="team">
        <% for(const dev of aboutTeam) {%>
            <li class="dev">
                <div class="dev-photo"> 
                    <img src=<%= dev.photo %>/>
                </div>
                <div class="dev-info">
                    <span><%= dev.name %></span>
                    <span><%= dev.email %></span>
                </div>
                
            </li>
            <% } %>
    </ul>
    <p class="license">Licen√ßa utilizada: GPLv3</p>
</div>
